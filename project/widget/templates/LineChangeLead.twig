<div class="pragma-line_change">

    <div class="line_change-delete-pragma">
        <div class="delete-pragma_icon">
            <button type="button" class="button-input    delete_el_card" tabindex="" id="delete_id_el_card">
                        <span class="button-input-inner ">
                            <svg width="18" height="18" viewBox="0 0 15 15" fill="none"
                                 xmlns="http://www.w3.org/2000/svg">
                                <path d="M10.5877 10.6252L3.5293 3.5415" stroke="red" stroke-width="0.6"
                                      stroke-linecap="round"></path>
                                <path d="M3.52949 10.6252L10.5879 3.5415" stroke="red" stroke-width="0.6"
                                      stroke-linecap="round"></path>
                            </svg>
                        </span>
            </button>
        </div>
        <div class="delete-pragma_text">
            {{ include_control('button', {
                class_name: "pragma_delete_text",
                id: "pragma_delete_"  ~ id,
                text: 'Удалить'
            }) }}
        </div>
    </div>

    <div class="line_change-sail_discount">
        <div class="sail_discount-icon">
            <svg xmlns="http://www.w3.org/2000/svg" xmlns:xlink="http://www.w3.org/1999/xlink" width="18" height="18" viewBox="0 0 18 18">
                <image width="18" height="18" xlink:href="data:img/png;base64,"/>
            </svg>
        </div>
        <div class="sail_discount-text">
            {{ include_control('button', {
                name: 'text',
                class_name: "sail_discount_text",
                id: "sail_discount_text" ~ id,
                text: 'Скидка'

            }) }}
        </div>
    </div>



</div>